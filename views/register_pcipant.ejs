    <!-- ======= Portfolio Section ======= -->
    <section id="portfolio" class="portfolio">
      <div class="container" data-aos="fade-up">
        <div class="section-title">
        <!-- <h2>Projects</h2> -->
          <p>Select Your Project</p> 
        </div>
       
        <div class="row" data-aos="fade-up" data-aos-delay="100">
          <div class="col-lg-12">
  
            <ul id="portfolio-flters">
              <p> Note: Read the guidelines properly before choosing you project.Having trouble? <a href="/#contact">Contact us</a> </p>
              <li class="filter-active">Choose Your Track >></li>
              <li data-filter=".filter-web">WEB-DEV</li>
              <li data-filter=".filter-ai">AI-ML</li>
              <li data-filter=".filter-app">APP-DEV</li>
              <li data-filter=".filter-iot">IOT/HARDWARE</li>
              <li data-filter=".filter-misc">OTHERS</li>
              <li data-filter=".filter-full">Registrations closed</li>
            </ul>
          </div>
        </div>
        <div
          class="row portfolio-container hide"
          data-aos="fade-up"
          data-aos-delay="200"
          id="irs"
        >
            <%  
              data.slice(0).reverse().map((result)=>{
                
                if(result.project_status == 'FULL')
                  sb ='full';
                else if(result.project_category == 'WEB-DEV' && result.project_status == 'YES')
                  sb = 'web';
                else if(result.project_category == 'APP-DEV' && result.project_status == 'YES')
                  sb = 'app';
                else if(result.project_category == 'AI-ML' && result.project_status == 'YES')
                  sb = 'ai';
                else if(result.project_category == 'IOT/HARDWARE' && result.project_status == 'YES')
                  sb = 'iot';
                else
                  sb = 'misc';

                  if(result.project_status == 'YES'){
            %>
            <div class="col-lg-4 col-md-6 portfolio-item filter-<%=sb%>">
               <div class="portfolio-wrap">
                 <img
                   src="projectimg/<%=result.image%>"
                   class="img-fluid"
                   alt=""
                 />
                 <div class="portfolio-links">
                   <a
                     href="projectimg/<%=result.image%>"
                     data-gallery="portfolioGallery"
                     class="portfolio-lightbox"
                     title="<div style='color:#eb5d1e; font-size:20px;'><b><%=result.project_nm%></b></div> <br> <%=result.project_info%> <br> Stacks: 
                     
                     <%
                        result.project_tech.split(',').map((e)=>{
                     %>
                          <span class='badge badge-primary'><%=e%></span>
                     <%   
                        })
                     %>
                     
                     <br> Github: <a href='<%=result.project_link%>' target='_blank'><%=result.project_link%></a>"
                     >
                     <i class="bi bi-plus"></i>
                     </a>
                   <!-- <a href="#" title="More Details"
                     ><i class="bi bi-link"></i
                   ></a> -->
                 </div>
                 <div class="portfolio-info">
                   <h4><%=result.project_nm%></h4>
                   <p><%=data.project_info%></p>
                 </div>
               </div>
     
               <div style="display: flex;
               justify-content: center;
               align-items: center;">
               <label>
                <input type="radio" id="project_select" name="pcipant_project" value="<%=result.project_nm%>" required>
                <span>Select this project</span>
              </label>
            </div>
            </div>
            
            <%
                  }

              if(result.project_status == 'FULL'){

            %>
            
                <div class="col-lg-4 col-md-6 portfolio-item filter-<%=sb%>">
               <div class="portfolio-wrap">
                 <img
                   src="projectimg/<%=result.image%>"
                   class="img-fluid"
                   alt=""
                 />
                 <div class="portfolio-links">
                   <a
                     href="projectimg/<%=result.image%>"
                     data-gallery="portfolioGallery"
                     class="portfolio-lightbox"
                     title="<div style='color:#eb5d1e; font-size:20px;'><b><%=result.project_nm%></b></div> <br> <%=result.project_info%> <br> Stacks: 
                     
                     <%
                        result.project_tech.split(',').map((e)=>{
                     %>
                          <span class='badge badge-primary'><%=e%></span>
                     <%   
                        })
                     %>
                     
                     <br> Github: <a href='<%=result.project_link%>' target='_blank'><%=result.project_link%></a>"
                     >
                     <i class="bi bi-plus"></i>
                     </a>
                   <!-- <a href="#" title="More Details"
                     ><i class="bi bi-link"></i
                   ></a> -->
                 </div>
                 <div class="portfolio-info">
                   <h4><%=result.project_nm%></h4>
                   <p><%=data.project_info%></p>
                 </div>
               </div>  
               </div>

            <%
              }      
              })
            %>
        </div>
      </div>
    </section>

